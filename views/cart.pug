extends layout
block body
	.navbar-wrapper
		.container
			include header
							
	.container
		.col-md-13
		+panel-default("Cart")
			table(style='width:100%')
				thead
					tr
						th Name
						th Quantity
						th Price
				tbody#cartItems


	include footer

block scripts
	script(src="/js/jquery.min.js")
	script(src="/js/bootstrap.min.js")
	script(src="/js/home.js")
	script(src="/js/cart.js")

