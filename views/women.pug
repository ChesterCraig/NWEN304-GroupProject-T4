extends layout

block body
    .navbar-wrapper
        .container
            include header

    .container
        .col-md-13
            .col-md-6
                h2 Womens Clothing

                each item in data
                    a(href = "items/"+item.id)
                        h4 #{item.name}
                    h6 #{item.description}
                    img(src = "" + item.image_path)


    include footer

block scripts
    script(src="/js/jquery.min.js")
    script(src="/js/bootstrap.min.js")
    script(src="/js/home.js")




